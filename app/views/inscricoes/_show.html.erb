    <div class="col-lg-8">
      <span class="fw-bold fs-6 text-gray-800"><%= resource.try(:nome_mae) %></span>
    </div>
  
</div>
<div class="row mb-7">
  
    <label class="col-lg-4 fw-semibold text-muted text-gray-800"><%= model_class.human_attribute_name(:rg) %> </label>
    <div class="col-lg-8">
      <span class="fw-bold fs-6 text-gray-800"><%= format_rg(resource.try(:rg)) %></span>
    </div>
  
</div>
<div class="row mb-7">
  
    <label class="col-lg-4 fw-semibold text-muted  text-gray-800"><%= model_class.human_attribute_name(:cpf) %> </label>
    <div class="col-lg-8">
      <span class="fw-bold fs-6 text-gray-800"><%=  format_cpf(resource.try(:cpf)) %></span>
    </div>
  
</div>
